    <style>
      main {
        height: 500px;
      }
      html,
      body {
        height: 100%;
      }

      body {
        display: flex;
        align-items: center;
        padding-top: 40px;
        padding-bottom: 40px;
        background-color: #f5f5f5;
      }

      .form-signin {
        max-width: 330px;
        padding: 15px;
      }

      .form-signin .form-floating:focus-within {
        z-index: 2;
      }

      .form-signin input[type="text"] {
        margin-bottom: -1px;
      }

      .form-signin input[type="password"] {
        margin-bottom: 10px;
      }
      .btn {
        text-decoration: none;
        }
      .btn-primary a{
        color: #f5f5f5;
      }
      .form-floating  ul{ 
        display: flex;
      }
      .form-floating li{
        list-style: none;
        padding-right: 10px;
        border-bottom: 1px solid #000000;
      }
      .form-floating  a {
        text-decoration: none;
        color: black;
      }
